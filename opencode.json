{
  "$schema": "https://opencode.ai/config.json",
  "model": "anthropic/claude-opus-4-5-high",
  "default_agent": "task-orchestrator",
  "small_model": "anthropic/claude-haiku-4-5",
  "permission": {
    "edit": "allow",
    "bash": "allow",
    "webfetch": "allow"
  },
  "plugin": [
    "oh-my-opencode",
    "opencode-openai-codex-auth",
    "opencode-antigravity-auth"
  ],
  "agent": {
    "build": {
      "tools": {
        "write": true,
        "edit": true,
        "read": true,
        "bash": true
      },
      "permission": {
        "edit": "allow",
        "bash": "allow"
      }
    },
    "general": {
      "tools": {
        "write": true,
        "edit": true,
        "read": true,
        "bash": true,
        "webfetch": true
      },
      "permission": {
        "edit": "allow",
        "bash": "allow",
        "webfetch": "allow"
      }
    },
    "executor": {
      "model": "anthropic/claude-opus-4-5-high",
      "tools": {
        "write": true,
        "edit": true,
        "read": true,
        "bash": true
      },
      "permission": {
        "edit": "allow",
        "bash": "allow"
      }
    }
  },
  "lsp": {
    "basedpyright": {
      "command": [
        "basedpyright",
        "--stdio"
      ],
      "extensions": [
        ".py",
        ".pyi"
      ]
    },
    "pyright": {
      "disabled": true
    }
  },
  "mcp": {
    "context7": {
      "command": [
        "bunx",
        "--bun",
        "-y",
        "@upstash/context7-mcp"
      ],
      "enabled": true,
      "environment": {},
      "type": "local"
    },
    "playwright": {
      "type": "local",
      "command": ["npx", "@playwright/mcp@latest"]
    },
  },
  "provider": {
    "google-vertex": {
      "models": {
        "gemini-2.5-pro": {
          "id": "gemini-2.5-pro-preview-06-05",
          "name": "Gemini 2.5 Pro (Vertex)",
          "limit": {
            "context": 1000000,
            "output": 65536
          }
        },
        "gemini-2.5-flash": {
          "id": "gemini-2.5-flash-preview-05-20",
          "name": "Gemini 2.5 Flash (Vertex)",
          "limit": {
            "context": 1000000,
            "output": 65536
          }
        }
      }
    },
    "anthropic": {
      "name": "Anthropic",
      "npm": "@ai-sdk/anthropic",
      "options": {
      },
      "models": {
        "claude-sonnet-4-5": {
          "id": "claude-sonnet-4-5-20250929",
          "name": "Sonnet 4.5",
          "limit": {
            "context": 200000,
            "output": 128000
          }
        },
        "claude-sonnet-4-5-high": {
          "id": "claude-sonnet-4-5-20250929",
          "name": "Sonnet 4.5 High",
          "limit": {
            "context": 200000,
            "output": 128000
          },
          "options": {
            "thinking": {
              "type": "enabled",
              "budgetTokens": 64000
            }
          }
        },
        "claude-opus-4-5": {
          "id": "claude-opus-4-5-20251101",
          "name": "Opus 4.5",
          "limit": {
            "context": 200000,
            "output": 128000
          }
        },
        "claude-opus-4-5-high": {
          "id": "claude-opus-4-5-20251101",
          "name": "Opus 4.5 High",
          "limit": {
            "context": 200000,
            "output": 128000
          },
          "options": {
            "thinking": {
              "type": "enabled",
              "budgetTokens": 64000
            }
          }
        }
      }
    },
    "google": {
      "name": "Google",
      "npm": "@ai-sdk/google",
      "models": {
        "antigravity-gemini-3-pro-low": {
          "name": "Gemini 3 Pro Low (Antigravity)",
          "limit": { "context": 1048576, "output": 65535 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
